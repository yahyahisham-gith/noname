-- Admin-Hub-V1: Forced Update Launcher
local target = "https://raw.githubusercontent.com/yahyahisham-gith/noname/main/admin-hub-V1"
local cacheBuster = "?update=" .. tick() .. "&ver=2.0.0"

local code = game:HttpGet(target .. cacheBuster)

if string.find(code, "VERSION: 2.0.0") then
    loadstring(code)()
else
    warn("Still pulling old cache! Restart Xeno.")
end
