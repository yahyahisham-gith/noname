-- Admin-Hub-V1: Dynamic Version Loader
-- Targets: Latest GitHub Commit (V1.9.2+)

local HttpService = game:GetService("HttpService")
local GitHubRaw = "https://raw.githubusercontent.com/yahyahisham-gith/noname/main/admin-hub-V1"

-- Generates a unique string to force GitHub to bypass cache
local cacheBuster = "?v=" .. HttpService:GenerateGUID(false) .. "&t=" .. tick() .. "&r=" .. math.random(1, 100000)

local success, result = pcall(function()
    return game:HttpGet(GitHubRaw .. cacheBuster)
end)

if success then
    print("Admin Hub: Successfully fetched latest version.")
    loadstring(result)()
else
    warn("Admin Hub: Failed to fetch update. Check connection.")
end
